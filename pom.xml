  <build>
    <sourceDirectory>target/src</sourceDirectory>
    <testSourceDirectory>src/test</testSourceDirectory>
    <testResources>
      <testResource>
        <targetPath/>
        <directory>conf/test</directory>
        <includes>
          <include>commons-logging.properties</include>
        </includes>
      </testResource>
    </testResources>
    <plugins>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <configuration>
          <includes>
            <include>**/*Test.java</include>
            <include>**/Test*.java</include>
          </includes>
          <excludes>
            <exclude>**/test/*.java</exclude>
          </excludes>
        </configuration>
      </plugin>
<!--      <plugin>-->
<!--        <groupId>org.apache.torque</groupId>-->
<!--        <artifactId>maven-torque-plugin</artifactId>-->
<!--        <version>3.3</version>-->
<!--        <configuration/>-->
<!--      </plugin>-->
